# Maintainer: Guten Ye

pkgname="saber-server-daemon"
pkgver=0.1
pkgrel=3
pkgdesc="a saber server daemon"
arch=("i686" "x86_64")
url="https://github.com/GutenYe/saber"
license=("MIT")
source=("rc.saber-server")

package() {
  cd "$srcdir"

  install -Dm755 rc.saber-server "$pkgdir/etc/rc.d/saber-server"
}

